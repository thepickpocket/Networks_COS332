<?xml version="1.0"?>
<!DOCTYPE wml PUBLIC "-//WAPFORUM//DTD WML 1.2//EN"
        "http://www.wapforum.org/DTD/wml12.dtd">
<wml>
    <card id="one" title="Edit Page">
        <p>
            <big><b>Edit Page</b></big>
                Name: <input name="name" size="12" value=""/>
                <br></br>
                New Telephone Number :  <input name="num" size="12" value=""/>
                <br></br>
                <anchor>
                    <go method="get" href="">
                        <postfield name="name" value="$(name)"/>
                        <postfield name="num" value="$(num)"/>
                    </go>
                    Update
                </anchor>
            <br></br>
            <a href="index.wml">Back</a>
        </p>
    </card>
</wml>